@*Reupero el viewbag*@
@*para referenciar un metodo, una variable o un elemento de codigo '@'*@
<h1>@ViewBag.Titulo</h1>
<br />
<div class="row">
    <div class="col-md-2" style="text-align:right;">
        <a href="~/Calificacion/Nueva_Calificacion" class="btn btn-primary">Nueva Calificacion</a>
    </div>
</div>
<br />
<div class="col-md-12 row text-center">
    @foreach (var calificacion in Model)
    {
        //Tarjeta de bootstrap
        <div class="col-md-3 col-sm-12">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">@calificacion.ID_Asignatura</h5>
                    <h5 class="card-title">@calificacion.ID_Estudiante</h5>
                    <h2 class="card-title">Num. @calificacion.ID_Calificacion</h2>
                    <h5 class="card-title">@calificacion.Calificacion1</h5>
                    <a class="btn btn-warning" href="~/Calificacion/Editar_Calificacion/@calificacion.ID_Calificacion">Editar</a>
                    <a class="btn btn-danger" href="~/Calificacion/Confirmar_Eliminar/@calificacion.ID_Calificacion">Eliminar</a>
                </div>
            </div>
        </div>
    }
</div>